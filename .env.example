# ============================================================================
# Voice Agent Configuration Template
# ============================================================================
# Copy this file to .env and fill in your actual API keys
# ============================================================================

# ----------------------------------------------------------------------------
# Speech-to-Text (STT) - Cartesia Ink-Whisper
# ----------------------------------------------------------------------------
# Sign up: https://cartesia.ai
# Get API key from: https://play.cartesia.ai/console
CARTESIA_API_KEY=your_cartesia_api_key_here

# ----------------------------------------------------------------------------
# Large Language Model (LLM) - Cerebras
# ----------------------------------------------------------------------------
# Sign up: https://cloud.cerebras.ai
# Get API key from: https://cloud.cerebras.ai/api-keys
CEREBRAS_API_KEY=your_cerebras_api_key_here

# ----------------------------------------------------------------------------
# Text-to-Speech (TTS) - ElevenLabs
# ----------------------------------------------------------------------------
# Sign up: https://elevenlabs.io
# Get API key from: https://elevenlabs.io/app/settings/api-keys
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# ----------------------------------------------------------------------------
# Transport Layer Configuration
# ----------------------------------------------------------------------------
# Options: "websocket" or "daily"
# - websocket: Simple setup, no external dependencies, higher latency (200-400ms)
# - daily: Production-grade WebRTC, ultra-low latency (<100ms), requires Daily account
TRANSPORT_TYPE=websocket

# ----------------------------------------------------------------------------
# Daily WebRTC Configuration (only needed if TRANSPORT_TYPE=daily)
# ----------------------------------------------------------------------------
# Sign up: https://dashboard.daily.co
# Get API key from: https://dashboard.daily.co/developers
# DAILY_API_KEY=your_daily_api_key_here
# DAILY_ROOM_URL=https://your-domain.daily.co/your-room-name
# DAILY_TOKEN=your_meeting_token_here

# ----------------------------------------------------------------------------
# WebSocket Configuration (only needed if TRANSPORT_TYPE=websocket)
# ----------------------------------------------------------------------------
WS_HOST=localhost
WS_PORT=8765

# ----------------------------------------------------------------------------
# Logging Configuration
# ----------------------------------------------------------------------------
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# ----------------------------------------------------------------------------
# Voice Agent Configuration
# ----------------------------------------------------------------------------
# ElevenLabs voice ID (default: Rachel)
# Find more voices: https://elevenlabs.io/voice-library
ELEVENLABS_VOICE_ID=21m00Tcm4TlvDq8ikWAM

# LLM Model (Cerebras)
# Options: llama-3.1-8b, llama-3.3-70b, llama-3.1-70b, llama-3.1-405b
LLM_MODEL=llama-3.3-70b

# LLM Temperature (0.0 - 1.0, higher = more creative)
LLM_TEMPERATURE=0.7

# Maximum tokens per response (for voice, keep brief: 100-200)
LLM_MAX_TOKENS=150

# Audio sample rate (Hz) - 16000 recommended for voice
AUDIO_SAMPLE_RATE=16000

# TTS latency optimization level (0-4, 3 recommended)
TTS_OPTIMIZE_LATENCY=3
